{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "frontend/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.8"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/app.py"
    }
  ],
  "env": {
    "PORT": "${{secrets.PORT}}",
    "PYTHONUNBUFFERED": "1"
  },
  "installCommand": "python3.8 -m pip install --upgrade pip && python3.8 -m pip install -r requirements.txt",
  "outputDirectory": "dist"}
}
