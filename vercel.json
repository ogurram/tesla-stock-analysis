{
  "version": 2,
  "builds": [
    {
      "src": "frontend/app.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/app.py"
    }
  ],
  "env": {
    "PORT": "${{secrets.PORT}}",
    "PYTHONUNBUFFERED": "1"
  },
  "installCommand": "pip install --upgrade pip && pip install -r requirements.txt"
}
